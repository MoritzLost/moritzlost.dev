{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "allowJs": true,
        "baseUrl": ".",
        "outDir": "dist",
        "paths": {
            "@components/*": [
                "src/components/*"
            ],
            "@data/*": [
                "src/data/*"
            ],
            "@layouts/*": [
                "src/layouts/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
        }
    }
}
